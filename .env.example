# 七牛云配置
QINIU_ACCESS_KEY=your_access_key_here
QINIU_SECRET_KEY=your_secret_key_here
QINIU_BUCKET_NAME=your_bucket_name_here
QINIU_DOMAIN=your_domain_here

# 上传配置
UPLOAD_FOLDER=static/uploads
QRCODE_FOLDER=static/qrcodes
MAX_CONTENT_LENGTH=104857600

# 七牛云上传token过期时间（秒）
QINIU_TOKEN_EXPIRE_TIME=3600

# Flask配置
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# 七牛云私有URL过期时间（秒）
QINIU_PRIVATE_URL_EXPIRE_TIME=3600
